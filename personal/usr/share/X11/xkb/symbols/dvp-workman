    key <AC08> {  [   e,  E   ] };
    key <AC09> {  [   o,  O   ] };
    key <AC10> {  [   i,  I   ] };
    key <AC11> { [ apostrophe,      quotedbl,       dead_acute                  ] };


    key <AB01> { [ comma,           less,           guillemotleft               ] };
    key <AB02> { [ period,          greater,        guillemotright              ] };
    key <AB03> {  [   v,  V   ] };
    key <AB04> {  [   c,  C   ] };
    key <AB05> { [ minus,           underscore,     hyphen                      ], type[Group1] = "FOUR_LEVEL_ALPHABETIC" };
    key <AB06> {  [   w,  W   ] };
    key <AB07> {  [   l,  L   ] };
    key <AB08> {  [   h,  H   ] };
    key <AB10> {  [   z,  Z   ] };
    key <AB09> {  [   j,  J   ] };
    key <BKSL> { [ backslash,       bar                                         ] };

